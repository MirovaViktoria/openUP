{
  "expo": {
    "name": "OpenUp",
    "slug": "open-up",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/c-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/c-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/c-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.openup.workoutapp"
    },
    "web": {
      "favicon": "./assets/c-icon.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "b6395af5-0273-4e28-acc2-7855037e1f1d"
      }
    }
  }
}
